mixin HeaderClassicSubmenu()
	ul.header-classic__submenu(data-subtext='Blog')
		li.header-classic__subitem
			a.header-classic__sublink(
				href="https://www.iq.com.br/newsletter"
				target="_blank"
				rel="noopener"
			) Newsletter

		li.header-classic__subitem
			a.header-classic__sublink(
				href="https://www.iq.com.br/conteudo"
				target="_blank"
				rel="noopener"
			) Contas

		li.header-classic__subitem
			a.header-classic__sublink(
				href="https://www.iq.com.br/financas-pessoais"
				target="_blank"
				rel="noopener"
			) Finanças pessoais

		li.header-classic__subitem
			a.header-classic__sublink(
				href="https://www.iq.com.br/cartoes/conteudo"
				target="_blank"
				rel="noopener"
			) Cartões de crédito
