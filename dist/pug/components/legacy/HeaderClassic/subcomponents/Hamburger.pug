mixin HeaderClassicHamburger()
	button#hamb.header-classic__hamburger
		div.header-classic__hamburger-line(
			class="header-classic__hamburger-line--one"
		)
		div.header-classic__hamburger-line(
			class="header-classic__hamburger-line--two"
		)
		div.header-classic__hamburger-line(
			class="header-classic__hamburger-line--three"
		)
