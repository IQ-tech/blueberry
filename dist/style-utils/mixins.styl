aboveXsmall()
	@media (min-width breakpoints-xsmall)
		{block}
aboveSmall()
	@media (min-width breakpoints-small)
		{block}
aboveMedium()
	@media (min-width breakpoints-medium)
		{block}
aboveLarge()
	@media (min-width breakpoints-large)
		{block}
aboveXLarge()
	@media (min-width breakpoints-xlarge)
		{block}
betweenAB(bpA, bpB)
	condition = '(min-width: %s) and (max-width: %s)' % (bpA bpB)
	@media condition
		{block}
elementReset()
	box-sizing border-box
	padding-left 0
	padding-right 0
	padding-top 0
	padding-bottom 0
	margin-left 0
	margin-right 0
	margin-top 0
	margin-bottom 0
	text-decoration none
componentDeepReset()
	elementReset()
	*
		elementReset()
	*:before
		elementReset()
	*:after
		elementReset()
	ul, li
		list-style none
