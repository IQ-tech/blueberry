mixin FooterInfo()
	.footer-classic__info
		.footer-classic__container(
			class="footer-classic__container--info"
		)
			.footer-classic__info-left
				.footer-classic__info-block
					span Iq360 Servicos de Informacao e Tecnologia Ltda.
					span CNPJ: 30.247.128/0001-15

				.footer-classic__info-block
					span Rua Fidêncio Ramos, 308 - Torre A - 5º andar - Vila Olímpia São Paulo, SP, Brasil
			
			.footer-classic__info-right
				.footer-classic__info-block(
					class="footer-classic__info-block--contact"
				)
					span
						strong Atendimento
					span contas.contato@iq.com.br

				.footer-classic__info-block(
					class="footer-classic__info-block--copy"
				)
					span &copy; - iq - Todos os direitos reservados