{
	"name": "iq-design-system",
	"version": "1.0.0",
	"description": "iq design system/ui library",
	"main": "dist/main.js",
	"types": "dist/types/index.d.ts",
	"scripts": {
		"bundle": "webpack -c config/webpack.prod.js",
		"generate:types": "tsc --emitDeclarationOnly",
		"build:lib": "gulp build-lib && yarn generate:types",
		"dev:react": "node_modules/@storybook/react/bin/index.js -p 6006 -c .storybook/react",
		"dev:pug": "node_modules/@storybook/html/bin/index.js -p 6008 -c .storybook/pug",
		"build-storybook:react": "node_modules/@storybook/react/bin/build.js -c .storybook/react -o build/",
		"build-storybook:pug": "node_modules/@storybook/html/bin/build.js -c .storybook/pug -o build/static-version",
		"build-storybook": "yarn build-storybook:react && yarn build-storybook:pug"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RedVentures/iq-design-system.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/RedVentures/iq-design-system/issues"
	},
	"homepage": "https://github.com/RedVentures/iq-design-system#readme",
	"devDependencies": {
		"@babel/cli": "^7.12.1",
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.5",
		"@babel/preset-typescript": "^7.12.1",
		"@storybook/addon-actions": "^6.1.14",
		"@storybook/addon-essentials": "^6.1.14",
		"@storybook/addon-links": "^6.1.14",
		"@storybook/html": "^6.1.14",
		"@storybook/react": "^6.1.14",
		"@types/react": "^16.9.55",
		"@types/react-dom": "^16.9.9",
		"babel-loader": "^8.1.0",
		"css-loader": "^5.0.0",
		"cssnano": "^4.1.10",
		"file-loader": "^6.2.0",
		"gulp": "^4.0.2",
		"mini-css-extract-plugin": "^1.1.1",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"poststylus": "^1.0.1",
		"pug": "2.0.4",
